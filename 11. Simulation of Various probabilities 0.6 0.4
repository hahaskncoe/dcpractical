px={.3 .25 .45];

pygx=[0.9 .1 0; 0 0.8 0.2;0 .3 .7]; 
for i=1:3
pxy(i,:)=px(i).*pygx(i,:);
end
py=[.27 .365 .365];
hx=0;
for i=1:3 
   hx=hx+px(i)*(log(1/px(i))/log(2));
end
hy=0;
for i=1:3
   hy=hy+py(i)*(log(1/py(i))/log(2));
end 
hxy=0;
for i=1:3
   for j=1:3
      if(pxy(i,j)~=0)
         hxy=hxy+pxy(i,j)*(log(1/pxy(i,j))/log(2));
      end
   end
end 
hygx=hxy-hx
hxgy=hxy-hy
ixy=hx-hxgy
